{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist", "node_modules", ".astro"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@assets/*": ["src/assets/*"],
      "@contentsubmodule/*": ["aaronmoreycom_content/content/*"],
      "@fontawesomesubmodule/*": ["Font-Awesome/*"],
      "@utils/*": ["src/utils/*"],
      "@schemas/*": ["src/schemas/*"]
    }
  }
}
